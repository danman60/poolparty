{
  "ci": {
    "collect": {
      "url": [
        "http://localhost:3000/",
        "http://localhost:3000/wallet"
      ],
      "startServerCommand": "npm run start",
      "startServerReadyPattern": "started server on",
      "numberOfRuns": 1
    },
    "assert": {
      "assertions": {
        "categories:performance": ["error", {"minScore": 0.85}]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}

